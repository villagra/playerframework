      <Log Type="ClipEvent" Id="VideoClipStarted" Conditions="ClipEventType=Started">
        <Value Name="Type" Id="typ" />
        <Value Name="LogId" Id="lid" />
        <Value Name="RelatedLogId" Id="rid" Optional="1" />
